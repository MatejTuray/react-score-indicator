{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react",
    "airbnb"
  ],
  "env": {
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": 0,
    "no-plusplus": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-boolean-value": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0
  }
}
